#! NX/KF 4.0
DefClass:<T_NAME> (%ug_create_instance ug_base_part);
# Set of default parameters for the part

# Point(x, y, z)
(List) testList: {
<POINTS>
};

(List) children: children() + {list1:};


(List) list1: loop
{
  for $i from 1 to length(testList:);
    if ($i < length(testList:))
        append {ug_createInstance(
            {
                class, ug_line;
                start_point, nth($i, testList:);
                end_point, nth($i+1, testList:);
            }
        )};
};